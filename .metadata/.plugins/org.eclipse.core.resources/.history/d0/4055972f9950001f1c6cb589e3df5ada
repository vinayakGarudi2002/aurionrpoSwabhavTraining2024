package com.aurionpro.behavioural.state.model;

public class Shipped  implements IPacketState {

	@Override
	public void next(IPacketState packet) {
		
		packet = new Delivered();
		System.out.println("packet moved to Shipped State ");
	
		
	}

	@Override
	public void current(IPacketState packet) {
		System.out.println("packet is in Ordered state");
		
	}

	@Override
	public void previous(IPacketState packet) {
		  System.out.println("No previous State");
		  return;
		
	}

}
